{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ApprovedProductDto",
  "description": "A single approved product",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "program": {
      "type": "object",
      "$ref": "program.json",
      "title": "program"
    },
    "orderable": {
      "type": "object",
      "$ref": "orderable.json",
      "title": "orderable"
    },
    "facilityType": {
      "type": "object",
      "$ref": "facilityType.json",
      "title": "facilityType"
    },
    "maxPeriodsOfStock": {
      "type": "number",
      "title": "maxPeriodsOfStock"
    },
    "minPeriodsOfStock": {
      "type": "number",
      "title": "minPeriodsOfStock"
    },
    "emergencyOrderPoint": {
      "type": "number",
      "title": "emergencyOrderPoint"
    },
    "version": {
      "type": "object",
      "$ref": "versioning.json",
      "description": "The version metadata. The field can only be empty when a new FTAP is created."
    }
  },
  "required": [
    "id",
    "program",
    "orderable",
    "facilityType",
    "maxPeriodsOfStock"
  ]
}
